<?xml version="1.0" encoding="UTF-8"?>
<neuroml xmlns="http://www.neuroml.org/schema/neuroml2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.neuroml.org/schema/neuroml2  https://raw.github.com/NeuroML/NeuroML2/development/Schemas/NeuroML2/NeuroML_v2beta3.xsd" id="net">
  <include href="kd.channel.nml"/>
  <include href="pas.channel.nml"/>
  <include href="ik_test.cell.nml"/>

  <pulseGenerator id="iclamp0" delay="100 ms" duration="100 ms" amplitude="-2nA"/>
  <pulseGenerator id="iclamp1" delay="200 ms" duration="200 ms" amplitude="2 nA"/>

  <network id="net" type="networkWithTemperature" temperature="24 degC">

    <population id="pop" component="cell" type="populationList" size="1">
      <instance id="0">
        <location x="0" y="0" z="0"/>
      </instance>
    </population>

    <inputList id="Clamps" component="iclamp0" population="pop">
      <input id="0" target="../pop/0/cell" destination="synapses"/>
    </inputList>
    <inputList id="Clamps" component="iclamp1" population="pop">
      <input id="1" target="../pop/0/cell" destination="synapses"/>
    </inputList>

  </network>

</neuroml>
